    "def show_mult_times(file):\n",
    "    data = pd.read_csv(file)\n",
    "    print(f'matrix size: {data[\"Size\"][0]}')\n",
    "    data = data.transpose().drop('Size').rename(columns={0: \"times[0]\"})\n",
    "\n",
    "    \n",
    "    data.plot(\n",
    "        kind='bar', \n",
    "        xlabel='loop order', \n",
    "        ylabel='multiplication time [s]', \n",
    "        figsize=(10, 5),\n",
    "        colormap='Set3',\n",
    "        legend=None\n",
    "    )\n",
    "    \n",
    "    return data\n",
    "    \n",
    "show_mult_times(\"matmul_times.csv\")"
   ]
  },
